<h1>会員一覧</h1>
  <p>管理者：石津谷</p>
  <% @user.each do |user| %>
    <div>
      <p>
        No.<%= user.id %>
        <%= link_to user.name, edit_admin_user_path(user.id) %>
        <% if user.is_deleted == false %>
          有効
        <% else %>
          退会
        <% end %>
      </p>
    </div>
  <% end %>

