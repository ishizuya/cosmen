<h1>レビュー履歴一覧</h1>
  <% @reviews.each do |review| %>
    <div>
      　<%= review.created_at.strftime('%Y/%m/%d %H:%M:%S') %> <%= review.item.brand %> <%= link_to review.item.name, admin_item_path(review.item_id) %>　<%= link_to review.user.name,  edit_admin_user_path(review.user_id) %>　<%= link_to "詳細", admin_review_path(review.id) %>
    </div>
  <% end %>
